  "scripts": {
    "build-dev": "ember build --environment=testing",
    "build": "ember build --environment=production",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:css": "stylelint --allow-empty-input \"**/*.css\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:js:fix": "eslint . --fix",
    "lint:js": "eslint . --cache",
    "lint:types": "tsc --noEmit",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "postinstall": "husky install",
    "preinstall": "npx only-allow pnpm",
    "release:major": "standard-version --tag-prefix=\"\" --commit-all --release-as major",
    "release:minor": "standard-version --tag-prefix=\"\" --commit-all --release-as minor",
    "release:patch": "standard-version --tag-prefix=\"\" --commit-all --release-as patch",
    "release": "standard-version --commit-all --tag-prefix=\"\"",
    "start": "ember serve",
    "test:ember": "ember test",
    "test": "npm-run-all test:*"
  },
  "standard-version": {
    "scripts": {}
