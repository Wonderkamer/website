{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":pinAllExceptPeerDependencies"],
  "rebaseWhen": "conflicted",
  "baseBranches": ["develop"],
  "hostRules": [
    {
      "hostType": "git-tags",
      "matchHost": "https://github.com/Wonderkamer",
      "encrypted": {
        "token": ""
      }
    },
    {
      "hostType": "docker-compose",
      "matchHost": "https://ghcr.io/Wonderkamer",
      "username": "basz",
      "encrypted": {
        "token": ""
      }
    },
    {
      "hostType": "docker",
      "matchHost": "https://ghcr.io/Wonderkamer",
      "username": "basz",
      "encrypted": {
        "token": ""
      }
    },
    {
      "hostType": "dockerfile",
      "matchHost": "https://ghcr.io/Wonderkamer",
      "username": "basz",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "packageRules": [
    {
      "description": "Automerge non-major updates",
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ]
}
